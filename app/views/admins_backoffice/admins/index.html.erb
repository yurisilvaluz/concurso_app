<h1> Lista de Adminstradores</h1>

    <hr class="sidebar-divider">

<div class="table-responsive">                          
<table class="table table-bordered dataTable" id="dataTable" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
  <thead>
    <tr role="row">
      <th class="sorting sorting_asc" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" aria-sort="ascending" aria-label="Name: activate to sort column descending" style="width: 57.0057px;">#</th>
      <th class="sorting" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 61.0057px;">Email</th>
            <th class="sorting" tabindex="0" aria-controls="dataTable" rowspan="1" colspan="1" aria-label="Position: activate to sort column ascending" style="width: 61.0057px;"></th>
      </tr>
        </thead>
        <tbody>
<% @admins.each do |admin| %>
<tr class="odd">

    <td class="sorting_1"> <%= admin.id %> </td>
    <td> <%= admin.email %> </td>
    <td style= " width: 0px"> 

            <%= link_to edit_admins_backoffice_admin_path(admin), class: "btn btn-info btn-circle btn-sm" do %>
            <i class="fas fa-edit"></i>
            <% end %>
            <a href="#" class="btn btn-danger btn-circle btn-sm">
              <i class="fas fa-trash"> </i> </a>
    </td>

</tr>
    <% end %>
        </tbody>
    </table>
</div>